{
	"singleQuote": true,
    "tabWidth": 4,
	"trailingComma": "es5",
	"arrowParens": "avoid",
	"semi": false,
	"printWidth": 100,
	"svelteIndentScriptAndStyle": false,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
